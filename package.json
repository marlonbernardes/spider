{
  "name": "spider",
  "version": "1.0.0",
  "description": "Web crawler implementation for Monzo",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node -r ts-node/register ./src/index.ts",
    "db:reset": "node -r ts-node/register ./scripts/set-up-elasticsearch.ts",
    "start:watch": "nodemon",
    "test": "jest",
    "lint": "eslint ./ --config ./eslintrc.yml --ext .ts",
    "lint:help": "eslint --help"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marlonbernardes/spider.git"
  },
  "keywords": [
    "Monzo",
    "Web crawler",
    "Crawler",
    "Spider",
    "interview"
  ],
  "author": "Marlon Bernardes <marlonjb.cb@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marlonbernardes/spider/issues"
  },
  "homepage": "https://github.com/marlonbernardes/spider#readme",
  "dependencies": {
    "@elastic/elasticsearch": "^7.4.0",
    "axios": "^0.19.0",
    "cheerio": "^1.0.0-rc.3",
    "cli-table": "^0.3.1",
    "dotenv": "^8.2.0",
    "http-status": "^1.4.0",
    "inquirer": "^7.0.0",
    "ioredis": "^4.14.1",
    "kafkajs": "^1.11.0",
    "mime-types": "^2.1.24",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/cli-table": "^0.3.0",
    "@types/uuid": "^3.4.5",
    "@types/cheerio": "^0.22.13",
    "@types/inquirer": "^6.5.0",
    "@types/ioredis": "^4.0.18",
    "@types/jest": "^24.0.19",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.19.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start",
    "ext": "ts"
  }
}
